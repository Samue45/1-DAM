Algoritmo HolaMundo
	
	//Bloque de declaraciones de datos
	Definir  edadAlumno Como Entero 
	Definir solution Como Caracter
	
	//Entrada de datos
	Escribir "Introduzca su edad: " 
    Leer edadAlumno
	
	//Intrucciones o procedimientos de los datos
	Si edadAlumno >= 18 Entonces 
		solution = "Eres mayor de edad"
	SiNo
		solution = "Eres menor de edad"
	FinSi
	
	//Salida
	Escribir  solution 
	
FinAlgoritmo
